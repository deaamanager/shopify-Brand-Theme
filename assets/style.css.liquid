    width: 2px;
    margin-top: 145px;
    margin-left: -52px;
    background: rgba(255,255,255,.99);
}

.custom_showcase__1 a:before,
.custom_showcase__1 a:after {
    top: -51.5%;
    left: 34.5%;
}
.custom_showcase__2 a:before,
.custom_showcase__2 a:after {
    top: 18.5%;
    left: 70%;
}
.custom_showcase__3 a:before,
.custom_showcase__3 a:after {
    top: -78.5%;
    left: 85.5%;
}
.custom_showcase img {
  	width: 100%;
    -webkit-transform: translate(0%,0%) scale(1);
    -moz-transform: translate(0%,0%) scale(1);
    -ms-transform: translate(0%,0%) scale(1);
    transform: translate(0%,0%) scale(1);
    opacity: .6;
    {{ trans_fast }}
}
.custom_showcase a:hover img {
    opacity: .3;
    -webkit-transform: translate(1%,4%) scale(1.1);
    -moz-transform: translate(1%,4%) scale(1.1);
    -ms-transform: translate(1%,4%) scale(1.1);
    transform: translate(1%,4%) scale(1.1);
}

.custom_showcase a:hover:after,
.custom_showcase a:hover:before {
    top: 10%;
    left: 10%;
    height: 80%;
    -ms-transform: skewX(0deg);
    -webkit-transform: skewX(0deg);
    transform: skewX(0deg);
    transition-delay: 0s;
}
.custom_showcase a:hover:after {
    left: 90%;
    margin-top: 0;
    margin-left: 0;
}

.custom_showcase:after,
.custom_showcase:before {
    position: absolute;
    right: 55%;
    left: 55%;
    top: 20px;
    content: "";
    height: 6px;
    background: rgba(255,255,255,.5);
    z-index: 9;
    {{ trans_fast }}
    transition-delay: 0s;
}
.custom_showcase:before {
    top: auto;
    bottom: 20px;
    height: 2px;
    background: rgba(255,255,255,.99);
}
.custom_showcase:hover:after,
.custom_showcase:hover:before {
    right: 20%;
    left: 20%;
    transition-delay: .1s;
}



