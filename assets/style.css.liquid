.custom_showcase a:after,
.custom_showcase a:before {
    position: absolute;
    top: -64%;
    left: 34.5%;
    content: "";
    height: 350px;
    z-index: 9;
    -ms-transform: skewX(-26deg);
    -webkit-transform: skewX(-26deg);
    transform: skewX(-26deg);
    {{ trans_fast }}
    transition-delay: .1s;
}
.custom_showcase a:before {
    width: 6px;
    margin-top: 0;
    margin-left: 0;
    background: rgba(255,255,255,.5);
}
